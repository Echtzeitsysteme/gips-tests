package "gipsl.imports.supera"
import "platform:/resource/gipsl.imports.importmodel/model/Importmodel.ecore"

config {
	solver := GUROBI;
	timeLimit := true [value := 42.0];
	randomSeed := true [value := 73];
	presolve := auto;
	debugOutput := false;
}

rule mapGuestA {
	root: Root {
		-elements -> h
		-elements -> g
	}

	h: Host

	g: Guest {
		++ -host -> h
	}
	# h.resource >= g.demand
}
