package "gipsl.all.build.objbug"
import "platform:/resource/gipsl.all.build.model/model/Model.ecore"

config {  
	solver := GUROBI;
	timeLimit := true [value := 42.0];
	randomSeed := true [value := 0];
	presolve := true;
	debugOutput := false;
}

pattern p {
	host: SubstrateResourceNode
	vnode: VirtualResourceNode
}

mapping m to p;

function o with m {
	1 / (1 + 1)
//	1
}

objective : max {
	functions.o
}
